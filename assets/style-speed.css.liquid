:root {
--f-size-rem: {{settings.fsize_body_mb}}%;
--text: {{settings.color_body_text}};
--bg: {{settings.color_body_bg}};
--bg-box: {{settings.color_wrap_bg}};

--bs-primary: {{settings.color_primary}};
--bs-primary-rgb: {{settings.color_primary | color_to_rgb | remove: 'rgb(' | remove: ')'}};
--bs-primary-lighten: {{settings.color_primary | color_lighten: 7}};
--bs-secondary: {{settings.color_secondary}};
--bs-secondary-lighten: {{settings.color_secondary | color_lighten: 7}};
--bs-success: #a8c26e;
--bs-success-lighten: {{'#a8c26e' | color_lighten: 7}};
--alert: #b10001; 
--alert-lighten: {{'#b10001' | color_lighten: 7}};
--bs-dark: #222;
--bs-dark-lighten: {{'#222' | color_lighten: 7}};
--bs-info: #0dcaf0;
--bs-warning: #d26e4b;
--bs-danger: #b10001;
--bs-light: #f8f9fa;

{%-if settings.image_body_bg != blank -%}
--bg-body: url('{{settings.image_body_bg | img_url: 'master'}}');
{%-endif-%}
--bglazy: {{settings.color_pt_bground}};
--bglazy_gradient: linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0),linear-gradient(var(--bglazy) 100%,transparent 0);
/*{%-if settings.img_pt_bground != blank-%}
--imglazy: url({{settings.img_pt_bground | img_url: '240x'}});
{%-else-%}
--imglazy: url({{"Infinity-1.2s-68px-Color.svg" | asset_url}});
{%-endif-%}*/
{%- assign as_r = settings.aspect_ratio | default: settings.aspect_ratio_default -%}
{%-assign width = as_r | remove: ' ' | split: '/' | first | plus: 0.00-%}
{%-assign height = as_r | remove: ' ' | split: '/' | last | plus: 0.00-%}
--img_ar: {{height|divided_by: width | times: 100.00000000}}%;
{%- if settings.font_source == 'google' and settings.rules_google_font != blank -%}
{%- assign rules_google_font = settings.rules_google_font | strip_newlines | split: ';' -%}
{%- for rule in rules_google_font -%}
--f_family_{{forloop.index}}: {{rule | split: ': ' | last | split: ',' | first}};
--f_fallback_{{forloop.index}}: {{rule | split: ': ' | last | split: ',' | last}};
{%- endfor -%}
{%- else -%}
--f_family_1: {{settings.font_family_shopify_1.family}};
--f_fallback_1: {{settings.font_family_shopify_1.fallback_families}};
--f_family_2: {{settings.font_family_shopify_2.family}};
--f_fallback_2: {{settings.font_family_shopify_2.fallback_families}};
--f_family_3: {{settings.font_family_shopify_3.family}};
--f_fallback_3: {{settings.font_family_shopify_3.fallback_families}};
{%- endif -%}
{%- case settings.font_base -%}
{%- when '--f_family_1' -%}
--f_family: var(--f_family_1);
--f_fallback: var(--f_fallback_1);
{%- when '--f_family_2' -%}
--f_family: var(--f_family_2);
--f_fallback: var(--f_fallback_2);
{%- when '--f_family_3' -%}
--f_family: var(--f_family_3);
--f_fallback: var(--f_fallback_3);
{%- endcase -%}  
--f_weight: {{settings.fweight_body}};
--f_style: {{settings.fstyle_body}};
}
