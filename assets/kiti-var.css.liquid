:root{
	--color_body_text: {{ settings.color_body_text }};
	--priceColor: {{ settings.color_pr_price }};
  --swiper-theme-color: #333;
}

/* // COLOR */

.section-title h3{
  font-size: {{ settings.fsize_section_title_mb | times: 1.0 | divided_by: 16}}rem;
  font-weight: {{ settings.fweight_section_title }};
}
@media (min-width: 768px) {
	.section-title h3{
    {%- case settings.font_section_title_family -%}
      {%- when '--f_family_1' -%}
        --f_family: var(--f_family_1);
        --f_fallback: var(--f_fallback_1);
      {%- when '--f_family_2' -%}
        --f_family: var(--f_family_2);
        --f_fallback: var(--f_fallback_2);
      {%- when '--f_family_3' -%}
        --f_family: var(--f_family_3);
        --f_fallback: var(--f_fallback_3);
    {%- endcase -%}
	  font-size: {{ settings.fsize_section_title | times: 1.0 | divided_by: 16}}rem;
    font-family: var(--f_family), var(--f_fallback);
	}
}

.section-title .sub-title, dfn.sub-title, .box-sub-title{
  {%- case settings.font_section_stitle_family -%}
    {%- when '--f_family_1' -%}
      --f_family: var(--f_family_1);
      --f_fallback: var(--f_fallback_1);
    {%- when '--f_family_2' -%}
      --f_family: var(--f_family_2);
      --f_fallback: var(--f_fallback_2);
    {%- when '--f_family_3' -%}
      --f_family: var(--f_family_3);
      --f_fallback: var(--f_fallback_3);
  {%- endcase -%}
  font-family: var(--f_family), var(--f_fallback);
  font-size: {{ settings.fsize_section_stitle | times: 1.0 | divided_by: 16}}rem;
  font-style: {{ settings.fstyle_section_stitle }};
}
.product-page .entry-price{
  --priceColor: {{settings.color_prouct_page_price}};
  font-size: {{ settings.fsize_psingle_price | times: 1.0 | divided_by: 16}}rem;
}
.product-price.product-price-onsale ins {
  color: {{ settings.color_pr_price_sale }};
}
.product-price [class*="ProductVaries-"]{
  color: {{ settings.color_prvs_price }};
}

.onsale{
  background-color: {{ settings.color_pr_onsale }};
}
.onsale:before{
  border-color: {{ settings.color_pr_onsale }};
}
.onexclusive{
  background-color: {{ settings.color_pr_onsoldout }};
}
.onexclusive:before{
  border-color: {{ settings.color_pr_onsoldout }};
}
.onnew{
  background-color: {{ settings.color_pr_onnew }};
}
.onnew:before{
  border-color: {{ settings.color_pr_onnew }};
}

.kt_progress_bar .progress-bar {
  background-color: {{ settings.stock_bg_process | default:'#a94442' }};
}

.kt_countdown.style02 .block {
  color: {{ settings.stock_bg_process | default:'#a94442' }};
}

.kt_countdown.style06 .flip-top{
  color: {{ settings.stock_bg_process | default:'#a94442' }};
}

.kt_progress_bar .progress {
  background-color: {{ settings.stock_bg | default:'#e9ecef' }};
}

.spendFreeShip .progress {
  background-color: {{ settings.threshold_bg | default:'#9e9e9e' }};
}

.spendFreeShip .progress-bar {
  background-color: {{ settings.threshold_bg_process | default:'#a6c76c' }};
}

/* // PRODUCT SINGLE */
.product-page .entry-title,.title-sidebar h3{
  font-size: {{ settings.fsize_psingle_title | times: 1.0 | divided_by: 16}}rem;
  font-weight: {{ settings.fweight_psingle_title }};
  font-style: {{ settings.fstyle_psingle_stitle }};
  text-transform: {{ settings.ttranf_psingle_title }};
}
.product-single-background{
  background-color: {{ settings.color_prouct_page_bg }};
}

/* // HEADER */


.header-announcement p{
  color: var(--text);
}
.header-announcement p:hover{
  color: var(--text_hv);
}

.header-top a:not(.btn),
.header-top .kt--drop-title {
  color: var(--text);
}
.header-top .kt--drop-in a,
.header-top .kt--drop-in span,
.header-top .kt--drop-in p {
  color: var(--color_body_text);
}
.header-top a:not(.btn):hover,
.header-top a:hover > span,
.header-top a:hover .d-icon-angle-down,
.header-top .kt--drop-title:hover,
.header-top .socials a:hover i {
  color: var(--text_hv);
}

.category-menu .category-mn-i:hover>a{
  border-color: var(--text_hv);
}

.header .menu-toggle{
  color: var(--text);
}

/*.header .call-label:hover p,
.header .icon-link:hover .cartCost,
.header .icon-link:hover .cartCostTotal{
  color: var(--text_hv);
}*/

.header .mn-lv1 > a{
  color: var(--text);
}
.header .mn-lv1:hover > a,
.header .mn-lv1.show > a,
.header .mn-lv1.active > a{
  color: var(--text_hv);
}
.main-mn .special_link > a {
  --color_special_link: {{ settings.color_text_headerb }};
}
.main-mn .special_link > a:hover {
  --color_special_link: var(--color_hv, {{ settings.color_text_headerb_hv }}) !important;
}

.main-mn .sub-mn .mn-i:hover > a {
  --text: var(--bs-primary);
  color: var(--text);
}

.header .category-menu .category-mn-i:hover span.title{
  color: {{ settings.color_text_header_hv }}
}


