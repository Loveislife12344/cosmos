        "options": [
          {
            "value": "sidebar",
            "label": "Sidebar"
          },
          {
            "value": "horizontal",
            "label": "Horizontal"
          },
          {
            "value": "navigation",
            "label": "Navigation"
          },
          {
            "value": "off-canvas",
            "label": "Off-Canvas"
          },
          {
            "value": "toggle-sidebar",
            "label": "Toggle Sidebar"
          }
        ]
      }
    ],
    "blocks": [
      {
        "type": "filter_item",
        "name": "Filter By...",
        "settings": [
          { 
            "type": "text",
            "id": "title",
            "label": "Title",
            "default": "Filter By"
          },
          {
            "type": "header",
            "content": "Custom style"
