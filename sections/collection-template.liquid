          },
          {
            "value": "3",
            "label": "3"
          },
          {
            "value": "4",
            "label": "4"
          },
          {
            "value": "5",
            "label": "5"
          },
          {
            "value": "6",
            "label": "6"
          },
          {
            "value": "7",
            "label": "7"
          },
          {
            "value": "8",
            "label": "8"
          }
        ]
      },
      {
        "type": "select",
        "id": "layout",
        "label": "Paging layout",
        "options": [
          {
            "label": "Paging",
            "value": "default"
          },
          {
            "label": "Button loadmore",
            "value": "loadmore"
          },
          {
            "label": "Infinity load",
            "value": "infinity"
          }
        ],
        "default": "default"
      },
